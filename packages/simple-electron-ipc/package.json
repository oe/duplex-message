{
  "name": "simple-electron-ipc",
  "version": "1.1.7",
  "description": "an easy way to use electron ipc, get a response via promise, event support progress feedback",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.es.js",
  "scripts": {
    "build": "vite build",
    "predev": "tsc demo/*.ts --outDir demo/dist -t es5",
    "dev": "electron ./demo/dist/main.js"
  },
  "files": [
    "dist/*"
  ],
  "keywords": [
    "rpc",
    "duplex",
    "ipc",
    "duplex-message",
    "hub",
    "electron",
    "promise"
  ],
  "dependencies": {
    "duplex-message": "workspace:"
  },
  "devDependencies": {
    "electron": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "electron": "*"
  },
  "repository": "https://oe@github.com/oe/duplex-message.git",
  "author": "Saiya",
  "license": "MIT",
  "homepage": "https://github.com/oe/duplex-message/tree/master/packages/simple-electron-ipc"
}
